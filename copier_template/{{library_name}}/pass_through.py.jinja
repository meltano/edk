"""Passthrough shim for {{ extension_name }} extension."""
import sys

import structlog
from meltano.edk.logging import pass_through_logging_config
from {{library_name}}.extension import {{ extension_name }}


def pass_through_cli() -> None:
    """Pass through CLI entry point."""
    pass_through_logging_config()
    ext = {{extension_name}}()
    ext.pass_through_invoker(
        structlog.getLogger("{{ cli_prefix }}_invoker"),
        *sys.argv[1:] if len(sys.argv) > 1 else []
    )
